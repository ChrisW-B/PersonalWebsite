{
  "presets": [["@babel/env", { "targets": { "node": "current" } }], "@babel/react"],
  "plugins": ["emotion", "@babel/plugin-proposal-class-properties"],
  "env": {
    "production": {
      "plugins": [["transform-react-remove-prop-types", { "mode": "remove", "removeImport": true }]],
      "presets": [["@babel/env", { "targets": { "node": "current" }, "forceAllTransforms": true, "useBuiltIns": "usage" }], "@babel/react"]
    }
  },
  "overrides": [
    {
      "test": "./client",
      "presets": [["@babel/env", { "targets": { "browsers": ["> 5%"] } }]]
    }
  ]
}
