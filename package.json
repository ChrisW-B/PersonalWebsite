{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack -p --config ./webpack.production.js",
    "dev": "env NODE_ENV=development webpack-dev-server --config ./webpack.development.js",
    "lint:css": "stylelint './react/components/**/*style.js'",
    "lint:js": "eslint \"./**/*.{js,jsx}\" --ignore-pattern 'public/**/*.js'",
    "lint:js:fix": "eslint --fix \"./**/*.{js,jsx}\" --ignore-pattern 'public/**/*.js'",
    "prettier": "prettier \"./**/*.{js,jsx,gql,html,yml,json}\" --write",
    "prettier:check": "prettier './**/*.{js,jsx,json}' -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/**/*.js{x,}": [
      "npm run prettier:check",
      "npm run lint:js"
    ],
    "./src/**/*.{gql,html,yml,json}": [
      "npm run prettier:check"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "@emotion/styled": "^10.0.23",
    "@hot-loader/react-dom": "^16.10.2",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-http": "^1.5.16",
    "clean-webpack-plugin": "^3.0.0",
    "emotion": "^10.0.23",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "html-webpack-plugin": "^3.2.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "react-hot-loader": "^4.12.15",
    "react-icons": "^3.8.0",
    "react-remarkable": "^1.1.3",
    "react-transition-group": "^4.3.0",
    "stylelint-webpack-plugin": "^1.0.3",
    "webpack-cdn-plugin": "^3.2.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-default-from": "^7.5.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/react": "^16.9.11",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.23",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "eslint-plugin-unicorn": "^12.1.0",
    "file-loader": "^4.2.0",
    "graphql-mini-transforms": "^1.0.3",
    "husky": "^3.0.9",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prettier-plugin-import-sort": "0.0.3",
    "prettier-plugin-packagejson": "^2.0.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-no-unsupported-browser-features": "^3.0.2",
    "stylelint-order": "^3.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "svg-react-loader": "^0.4.6",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "react"
    }
  }
}
