{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "server/index.js",
  "scripts": {
    "prebuild": "npm run generate:graphql",
    "build": "npm run build:react && npm run build:funcs",
    "build:funcs": "env NODE_ENV=production netlify-lambda build ./src -c webpack.functions.js",
    "build:react": "env NODE_ENV=production webpack -p --config ./webpack.production.js",
    "generate:graphql": "graphql-codegen",
    "lint:css": "stylelint './**/styles/*.{j,t}s{x,}'",
    "lint:js": "eslint \"./**/*.{j,t}s{x,}\" --ignore-pattern \"public/**/*.js\"",
    "lint:js:fix": "eslint --fix \"./**/*.{j,t}s{x,}\" --ignore-pattern 'public/**/*.js'",
    "prettier": "prettier \"./**/*.{ts,tsx,js,jsx,gql,html,yml,json}\" --write",
    "prettier:check": "prettier './**/*.{ts,tsx,js,jsx,json}' -c",
    "prestart": "npm run generate:graphql",
    "start": "env NODE_ENV=development webpack-dev-server --config ./webpack.development.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{gql,html,yml,json}": [
      "prettier -c"
    ],
    "./src/**/*.{j,t}s{x,}": [
      "prettier -c",
      "eslint"
    ],
    "./src/App/styles/*.{j,t}s{x,}": [
      "prettier -c",
      "eslint",
      "stylelint"
    ]
  },
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@bundle-analyzer/webpack-plugin": "^0.5.1",
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@hot-loader/react-dom": "^16.11.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "emotion": "^10.0.27",
    "express": "^4.17.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "html-webpack-plugin": "^3.2.0",
    "intersection-observer": "^0.7.0",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "react": "^16.12.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.19",
    "react-intersection-observer": "^8.25.3",
    "react-markdown": "^4.3.1",
    "serverless-http": "^2.3.1",
    "stylelint-webpack-plugin": "^1.2.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cdn-plugin": "^3.2.2",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
