{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "src/server.js",
  "scripts": {
    "prebuild": "npm run generate:graphql",
    "build": "npm run build:react && npm run build:funcs",
    "build:funcs": "env NODE_ENV=production webpack --mode='production' -c ./webpack.functions.js",
    "build:react": "env NODE_ENV=production webpack --mode='production' -c ./webpack.production.js",
    "deploy": "zip-it-and-ship-it functions/ functions-dist/ && netlify deploy --functions=functions-dist",
    "generate:graphql": "graphql-codegen --config ./.graphqlcodegen.yml",
    "lint:css": "stylelint './**/styles/*.{j,t}s{x,}'",
    "prelint:js": "npm run generate:graphql",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx,.graphql,.gql --cache --cache-location 'node_modules/.cache/.eslintcache/'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --list-different \"./**/*.{ts,tsx,js,jsx,gql,html,yml,json}\" --write",
    "preprettier:check": "npm run generate:graphql",
    "prettier:check": "prettier './**/*.{ts,tsx,js,jsx,json}' -c",
    "prestart": "npm run generate:graphql",
    "start": "env NODE_ENV=development webpack serve -c ./webpack.development.js",
    "pretypescript": "npm run generate:graphql",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "3.3.13",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "cors": "2.8.5",
    "cross-fetch": "3.1.2",
    "express": "4.17.1",
    "graphql": "15.5.0",
    "graphql-tag": "2.11.0",
    "intersection-observer": "0.12.0",
    "node-fetch": "2.6.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-intersection-observer": "8.31.0",
    "react-markdown": "5.0.3",
    "serverless-http": "2.7.0"
  },
