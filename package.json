    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "babel-node",
    "dev": "nodemon --config ./config/nodemon.json --exec npm run babel -- ./server ",
    "build:server": "webpack -p --config ./config/webpack.server.prod.js",
    "build:client": "webpack -p --config ./config/webpack.client.prod.js",
    "prod": "npm run clean && npm run build:client && npm run build:server && npm run start",
