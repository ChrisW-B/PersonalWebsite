{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "server/index.js",
  "scripts": {
    "prebuild": "npm run generate:graphql",
    "build": "npm run build:react && npm run build:funcs",
    "build:funcs": "env NODE_ENV=production netlify-lambda build ./src -c webpack.functions.js",
    "build:react": "env NODE_ENV=production webpack -p --config ./webpack.production.js",
    "generate:graphql": "graphql-codegen  --config ./.graphqlcodegen.yml",
    "lint:css": "stylelint './**/styles/*.{j,t}s{x,}'",
    "prelint:js": "npm run generate:graphql",
    "lint:js": "eslint \"./**/*.{j,t}s{x,}\" --ignore-pattern \"public/**/*.js\"",
    "lint:js:fix": "eslint --fix \"./**/*.{j,t}s{x,}\" --ignore-pattern 'public/**/*.js'",
    "prettier": "prettier --list-different \"./**/*.{ts,tsx,js,jsx,gql,html,yml,json}\" --write",
    "preprettier:check": "npm run generate:graphql",
    "prettier:check": "prettier './**/*.{ts,tsx,js,jsx,json}' -c",
    "prestart": "npm run generate:graphql",
    "start": "env NODE_ENV=development webpack-dev-server --config ./webpack.development.js",
    "pretypescript": "npm run generate:graphql",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "@hot-loader/react-dom": "^16.14.0",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.3",
    "cors": "^2.8.5",
    "dotenv-webpack": "^6.0.0",
    "emotion": "^11.0.0",
    "express": "^4.17.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.10.3",
    "html-webpack-plugin": "^4.5.1",
    "intersection-observer": "^0.12.0",
    "node-fetch": "^2.6.1",
    "path": "^0.12.7",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-intersection-observer": "^8.31.0",
    "react-markdown": "^4.3.1",
    "serverless-http": "^2.7.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@graphql-codegen/cli": "^1.20.1",
    "@graphql-codegen/fragment-matcher": "^2.0.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/schema-ast": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.2",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.18.1",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@types/aws-lambda": "^8.10.71",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@typescript-eslint/typescript-estree": "^4.14.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-emotion": "^10.0.33",
    "encoding": "^0.1.13",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-csstyper": "^0.3.2",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "netlify-lambda": "^2.0.3",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-plugin-import-sort": "0.0.6",
    "prettier-plugin-packagejson": "^2.2.9",
    "raw-loader": "^4.0.2",
    "stylelint": "^13.9.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-csstree-validator": "^1.9.0",
    "stylelint-custom-processor-loader": "^0.6.0",
    "stylelint-no-unsupported-browser-features": "^4.1.4",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "svg-react-loader": "^0.4.6",
    "typescript": "^3.9.7",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
